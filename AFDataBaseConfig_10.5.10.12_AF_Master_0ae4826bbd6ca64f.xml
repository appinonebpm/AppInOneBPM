ge'                                                                                                         &#xD;&#xA;AND ec_field.fieldlabel != 'Description'                                                                                                           &#xD;&#xA;AND ec_field.fieldlabel != 'Modified Time'                                                                                                         &#xD;&#xA;AND ec_field.fieldlabel != 'Created Time'                                                                                                          &#xD;&#xA;AND ec_field.fieldlabel != 'Website';&#x9;  &#xD;&#xA;" Type="VIEW" Purview="0" /><View ViewName="v_ec_field_ec_productfieldlist" Sql="CREATE VIEW  v_ec_field_ec_productfieldlist &#xD;&#xA;AS                                                                                                           &#xD;&#xA;SELECT&#xD;&#xA;&#x9;ec_field.columnname,&#xD;&#xA;&#x9;ec_field.fieldlabel,&#xD;&#xA;&#x9;ec_productfieldlist.fieldname,&#xD;&#xA;&#x9;ec_productfieldlist.width,&#xD;&#xA;&#x9;ec_productfieldlist.module,&#xD;&#xA;&#x9;ec_field.fieldid&#xD;&#xA;FROM&#xD;&#xA;&#x9;ec_field&#xD;&#xA;INNER JOIN ec_productfieldlist ON ec_productfieldlist.fieldname = ec_field.columnname;&#xD;&#xA;" Type="VIEW" Purview="0" /><View ViewName="v_ec_field_ec_tab" Sql="CREATE VIEW  v_ec_field_ec_tab &#xD;&#xA;AS &#xD;&#xA;SELECT DISTINCT&#xD;&#xA;&#x9;ec_field.tabid,&#xD;&#xA;&#x9;NAME&#xD;&#xA;FROM&#xD;&#xA;&#x9;ec_field&#xD;&#xA;INNER JOIN ec_tab ON ec_field.tabid = ec_tab.tabid&#xD;&#xA;WHERE&#xD;&#xA;&#x9;ec_field.tabid NOT IN (7, 9, 10, 29)&#xD;&#xA;AND NAME NOT IN (&#xD;&#xA;&#x9;'Sfalists',&#xD;&#xA;&#x9;'Sfalogs',&#xD;&#xA;&#x9;'Sfasettings'&#xD;&#xA;);" Type="VIEW" Purview="0" /><View ViewName="v_ec_field_ec_tab_1" Sql="CREATE VIEW  v_ec_field_ec_tab_1 &#xD;&#xA;AS &#xD;&#xA;SELECT DISTINCT&#xD;&#xA;&#x9;ec_field.tabid,&#xD;&#xA;&#x9;NAME&#xD;&#xA;FROM&#xD;&#xA;&#x9;ec_field&#xD;&#xA;INNER JOIN ec_tab ON ec_field.tabid = ec_tab.tabid&#xD;&#xA;WHERE&#xD;&#xA;&#x9;ec_field.tabid NOT IN (7, 9, 10, 29)&#xD;&#xA;AND NAME NOT IN (&#xD;&#xA;&#x9;'Sfalists',&#xD;&#xA;&#x9;'Sfalogs',&#xD;&#xA;&#x9;'Sfasettings',&#xD;&#xA;&#x9;'Announcements'&#xD;&#xA;);" Type="VIEW" Purview="0" /><View ViewName="v_ec_modulelist_ec_tab" Sql="CREATE VIEW  v_ec_modulelist_ec_tab &#xD;&#xA;AS                                                                       &#xD;&#xA;SELECT&#xD;&#xA;&#x9;ec_modulelist.*, ec_tab. NAME&#xD;&#xA;FROM&#xD;&#xA;&#x9;ec_modulelist&#xD;&#xA;INNER JOIN ec_tab ON ec_tab.tabid = ec_modulelist.tabid&#xD;&#xA;WHERE&#xD;&#xA;&#x9;ec_modulelist.type = 'template'&#xD;&#xA;AND ec_tab. NAME NOT IN (&#xD;&#xA;&#x9;'Users',&#xD;&#xA;&#x9;'Events',&#xD;&#xA;&#x9;'Calendar',&#xD;&#xA;&#x9;'Downloads',&#xD;&#xA;&#x9;'Dashboard',&#xD;&#xA;&#x9;'Home',&#xD;&#xA;&#x9;'Webmails',&#xD;&#xA;&#x9;'Recyclebin',&#xD;&#xA;&#x9;'Rss',&#xD;&#xA;&#x9;'Portal',&#xD;&#xA;&#x9;'Reports',&#xD;&#xA;&#x9;'Workflows',&#xD;&#xA;&#x9;'Maillists',&#xD;&#xA;&#x9;'Qunfas',&#xD;&#xA;&#x9;'Messages',&#xD;&#xA;&#x9;'Emails',&#xD;&#xA;&#x9;'Leads',&#xD;&#xA;&#x9;'Balances',&#xD;&#xA;&#x9;'Funnels',&#xD;&#xA;&#x9;'Prdtnums',&#xD;&#xA;&#x9;'Bsteps',&#xD;&#xA;&#x9;'Fsteps',&#xD;&#xA;&#x9;'Isteps',&#xD;&#xA;&#x9;'Ssteps',&#xD;&#xA;&#x9;'Catalogs',&#xD;&#xA;&#x9;'Ureports',&#xD;&#xA;&#x9;'Performances',&#xD;&#xA;&#x9;'Inboundsearchs',&#xD;&#xA;&#x9;'Integradedreports',&#xD;&#xA;&#x9;'Dailylogs',&#xD;&#xA;&#x9;'Weeklylogs',&#xD;&#xA;&#x9;'Monthlylogs',&#xD;&#xA;&#x9;'Knowledgebases',&#xD;&#xA;&#x9;'Pools',&#xD;&#xA;&#x9;'Normaltasks',&#xD;&#xA;&#x9;'Serialnumbers',&#xD;&#xA;&#x9;'Productspecs',&#xD;&#xA;&#x9;'Prodlocations',&#xD;&#xA;&#x9;'Packsetups',&#xD;&#xA;&#x9;'Faqcategorys',&#xD;&#xA;&#x9;'Inventoryledgers',&#xD;&#xA;&#x9;'Forthamounts',&#xD;&#xA;&#x9;'Openingbalancess',&#xD;&#xA;&#x9;'Salesobjects',&#xD;&#xA;&#x9;'Salesreports',&#xD;&#xA;&#x9;'Companyacctreports',&#xD;&#xA;&#x9;'Aboutsfas',&#xD;&#xA;&#x9;'SfaDesktops',&#xD;&#xA;&#x9;'Sfalists',&#xD;&#xA;&#x9;'Sfalogs',&#xD;&#xA;&#x9;'Sfasettings'&#xD;&#xA;);                                                                        &#xD;&#xA;&#xD;&#xA;" Type="VIEW" Purview="0" /><View ViewName="v_ec_modulelist_ec_tab_1" Sql="CREATE VIEW  v_ec_modulelist_ec_tab_1 &#xD;&#xA;AS  &#xD;&#xA; SELECT&#xD;&#xA;&#x9;ec_modulelist.*, ec_tab. NAME&#xD;&#xA;FROM&#xD;&#xA;&#x9;ec_modulelist&#xD;&#xA;INNER JOIN ec_tab ON ec_tab.tabid = ec_modulelist.tabid;&#xD;&#xA;" Type="VIEW" Purview="0" /><View ViewName="v_ec_modules_ec_parenttab" Sql="CREATE VIEW  v_ec_modules_ec_parenttab &#xD;&#xA;AS SELECT&#xD;&#xA;&#x9;ec_modules.*, ec_parenttab.parenttab_label&#xD;&#xA;FROM&#xD;&#xA;&#x9;ec_modules&#xD;&#xA;LEFT JOIN ec_parenttab ON ec_parenttab.parenttabid = ec_modules.parenttabid;" Type="VIEW" Purview="0" /><View ViewName="v_ec_multifield_ec_tab" Sql="CREATE VIEW  v_ec_multifield_ec_tab &#xD;&#xA;AS &#xD;&#xA;SELECT&#xD;&#xA;&#x9;ec_multifield.*, ec_tab. NAME AS modulename&#xD;&#xA;FROM&#xD;&#xA;&#x9;ec_multifield&#xD;&#xA;INNER JOIN ec_tab ON ec_tab.tabid = ec_multifield.tabid;&#xD;&#xA;" Type="VIEW" Purview="0" /><View ViewName="v_ec_parenttabrel_ec_tab_ec_parenttab" Sql="CREATE VIEW  v_ec_parenttabrel_ec_tab_ec_parenttab &#xD;&#xA;AS SELECT&#xD;&#xA;&#x9;NAME,&#xD;&#xA;&#x9;tablabel,&#xD;&#xA;&#x9;parenttab_label,&#xD;&#xA;&#x9;ec_tab.tabid&#xD;&#xA;FROM&#xD;&#xA;&#x9;ec_parenttabrel&#xD;&#xA;INNER JOIN ec_tab ON ec_parenttabrel.tabid = ec_tab.tabid&#xD;&#xA;INNER JOIN ec_parenttab ON ec_parenttabrel.parenttabid = ec_parenttab.parenttabid;" Type="VIEW" Purview="0" /></DataBase>