e><View ViewName="v_ec_blocks" Sql="CREATE VIEW  v_ec_blocks AS SELECT&#xD;&#xA;&#x9;blockid,&#xD;&#xA;&#x9;blocklabel,&#xD;&#xA;&#x9;sequence&#xD;&#xA;FROM&#xD;&#xA;&#x9;ec_blocks&#xD;&#xA;WHERE&#xD;&#xA;&#x9;tabid NOT IN (7, 9, 10, 29)&#xD;&#xA;AND visible = 0;" Type="VIEW" Purview="0" /><View ViewName="v_ec_blocks_ec_field_ec_def_org_field" Sql="CREATE VIEW  v_ec_blocks_ec_field_ec_def_org_field &#xD;&#xA;AS &#xD;&#xA;SELECT&#xD;&#xA;&#x9;b.blockid,&#xD;&#xA;&#x9;b.tabid,&#xD;&#xA;&#x9;b.blocklabel,&#xD;&#xA;&#x9;b.sequence AS blocksequence,&#xD;&#xA;&#x9;v.sequence,&#xD;&#xA;&#x9;v.fieldlabel,&#xD;&#xA;&#x9;v.fieldid,&#xD;&#xA;&#x9;v.block,&#xD;&#xA;&#x9;v.typeofdata,&#xD;&#xA;&#x9;v.visible&#xD;&#xA;FROM&#xD;&#xA;&#x9;ec_blocks b,&#xD;&#xA;&#x9;(&#xD;&#xA;&#x9;&#x9;SELECT&#xD;&#xA;&#x9;&#x9;&#x9;ec_field.tabid,&#xD;&#xA;&#x9;&#x9;&#x9;ec_field.sequence,&#xD;&#xA;&#x9;&#x9;&#x9;ec_field.fieldlabel,&#xD;&#xA;&#x9;&#x9;&#x9;ec_field.fieldid,&#xD;&#xA;&#x9;&#x9;&#x9;ec_field.block,&#xD;&#xA;&#x9;&#x9;&#x9;ec_field.typeofdata,&#xD;&#xA;&#x9;&#x9;&#x9;ec_def_org_field.visible&#xD;&#xA;&#x9;&#x9;FROM&#xD;&#xA;&#x9;&#x9;&#x9;ec_field&#xD;&#xA;&#x9;&#x9;INNER JOIN ec_def_org_field ON ec_def_org_field.fieldid = ec_field.fieldid&#xD;&#xA;&#x9;&#x9;WHERE&#xD;&#xA;&#x9;&#x9;&#x9;ec_def_org_field.visible = 0&#xD;&#xA;&#x9;&#x9;AND displaytype IN (1, 2, 4)&#xD;&#xA;&#x9;) v&#xD;&#xA;WHERE&#xD;&#xA;&#x9;v.tabid = b.tabid&#xD;&#xA;AND v.block = b.blockid&#xD;&#xA;;" Type="VIEW" Purview="0" /><View ViewName="v_ec_def_org_field_ec_field" Sql="CREATE VIEW  v_ec_def_org_field_ec_field &#xD;&#xA;AS &#xD;&#xA;SELECT&#xD;&#xA;&#x9;ec_def_org_field.visible,&#xD;&#xA;&#x9;ec_field.*&#xD;&#xA;FROM&#xD;&#xA;&#x9;ec_def_org_field&#xD;&#xA;INNER JOIN ec_field ON ec_field.fieldid = ec_def_org_field.fieldid;&#xD;&#xA;" Type="VIEW" Purview="0" /><View ViewName="v_ec_field_ec_def_org_field_ec_logfieldlist_ec_logfieldlist" Sql="CREATE VIEW  v_ec_field_ec_def_org_field_ec_logfieldlist_ec_logfieldlist &#xD;&#xA;AS &#xD;&#xA;SELECT&#xD;&#xA;&#x9;ec_field.columnname,&#xD;&#xA;&#x9;ec_field.fieldlabel,&#xD;&#xA;&#x9;ec_logfieldlist.fieldname,&#xD;&#xA;&#x9;ec_logfieldlist.module,&#xD;&#xA;&#x9;ec_field.fieldid,&#xD;&#xA;&#x9;ec_field.sequence&#xD;&#xA;FROM&#xD;&#xA;&#x9;ec_field&#xD;&#xA;INNER JOIN ec_def_org_field ON ec_def_org_field.fieldid = ec_field.fieldid&#xD;&#xA;LEFT JOIN ec_logfieldlist ON ec_logfieldlist.fieldname = ec_field.columnname&#xD;&#xA;AND ec_logfieldlist.module = 'Contacts'&#xD;&#xA;WHERE&#xD;&#xA;&#x9;ec_def_org_field.visible = 0&#xD;&#xA;AND ec_field.uitype NOT IN (&#xD;&#xA;&#x9;10,&#xD;&#xA;&#x9;19,&#xD;&#xA;&#x9;69,&#xD;&#xA;&#x9;61,&#xD;&#xA;&#x9;30,&#xD;&#xA;&#x9;105,&#xD;&#xA;&#x9;76,&#xD;&#xA;&#x9;70,&#xD;&#xA;&#x9;79,&#xD;&#xA;&#x9;80,&#xD;&#xA;&#x9;1004,&#xD;&#xA;&#x9;1006,&#xD;&#xA;&#x9;1010&#xD;&#xA;)&#xD;&#xA;UNION&#xD;&#xA;&#x9;SELECT&#xD;&#xA;&#x9;&#x9;ec_field.columnname,&#xD;&#xA;&#x9;&#x9;ec_field.fieldlabel,&#xD;&#xA;&#x9;&#x9;ec_logfieldlist.fieldname,&#xD;&#xA;&#x9;&#x9;ec_logfieldlist.module,&#xD;&#xA;&#x9;&#x9;ec_field.fieldid,&#xD;&#xA;&#x9;&#x9;ec_field.sequence&#xD;&#xA;&#x9;FROM&#xD;&#xA;&#x9;&#x9;ec_field&#xD;&#xA;&#x9;LEFT JOIN ec_logfieldlist ON ec_logfieldlist.fieldname = ec_field.columnname&#xD;&#xA;&#x9;AND ec_logfieldlist.module = 'Contacts'&#xD;&#xA;&#x9;WHERE&#xD;&#xA;&#x9;&#x9;ec_field.columnname = 'total'&#xD;&#xA;&#x9;AND ec_field.displaytype = 3;" Type="VIEW" Purview="0" /><View ViewName="v_ec_field_ec_def_org_field_ec_productfieldlist" Sql="CREATE VIEW  v_ec_field_ec_def_org_field_ec_productfieldlist &#xD;&#xA;AS &#xD;&#xA;SELECT                                                                                                                                             &#xD;&#xA;&#x9;ec_field.columnname,                                                                                                                             &#xD;&#xA;&#x9;ec_field.fieldlabel,                                          