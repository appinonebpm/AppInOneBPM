   <script type="text/javascript">
       dhtmlx.skin = "dhx_skyblue";

       function createGrid(me) {
           var mygrid = new dhtmlXGridFromTable('tblToGrid');
           me.style.display = 'none';

           //mygrid.splitAt(4);
           //mygrid.setHeader("序号,车次,查询区间,#cspan,区间运行时刻,#cspan");
